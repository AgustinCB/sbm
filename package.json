{
  "name": "simple-blog-machine",
  "version": "1.0.0",
  "description": "Simple blog service",
  "main": "./lib/main",
  "bin": {
    "sbm": "./bin/sbm"
  },
  "scripts": {
    "start": "./start.sh",
    "test": "nodemon -i src --exec 'chmod +x test.sh; ./test.sh'",
    "gendoc": "./node_modules/jsdox/bin/jsdox --output docs src",
    "build": "./node_modules/babel-cli/bin/babel.js src --presets babel-preset-es2015 --out-dir lib",
    "watch": "nodemon --watch src --exec 'npm run build'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AgustinCB/npm-boilerplate.git"
  },
  "keywords": [
    "blog",
    "server"
  ],
  "author": "AgustinCB",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/AgustinCB/npm-boilerplate/issues"
  },
  "homepage": "https://github.com/AgustinCB/npm-boilerplate#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "jsdox": "^0.4.10",
    "mocha": "^2.5.3",
    "nodemon": "^1.11.0",
    "standard": "^7.1.2",
    "superagent": "^3.3.1"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "chai-http": "^3.0.0",
    "express": "^4.14.0",
    "express-jwt": "^5.1.0",
    "minimist": "^1.2.0",
    "mongoose": "^4.7.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.0.0",
    "promisify-node": "^0.4.0",
    "request": "^2.79.0"
  }
}
